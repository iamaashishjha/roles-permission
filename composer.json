{
    "name": "codenomads/roles-permission",
    "description": "Laravel 10.XX Roles and Permission complete",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "codenomads",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security"
    ],
    "autoload": {
        "psr-4": {
            "Codenomads\\RolesPermission\\": "src/"
        }
    },
    "authors": [
        {
            "name": "iamaashishjha",
            "email": "mail.aashishjha@gmail.com"
        }
    ],
    "homepage": "https://github.com/iamaashishjha/roles-permission.git",
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/container": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/passport": "^11.0",
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.4|^10.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "v1.0.0",

    "time": "2024-02-19 10:48 UTC",
    "support": {
        "email": "mail.aashishjha@gmail.com",
        "issues": "https://github.com/iamaashishjha/roles-permission.git",
        "source": "https://github.com/iamaashishjha/roles-permission.git"
    },
    "config": {
        "platform": {
            "php": "7.4.3"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Codenomads\\RolesPermission\\RolesPermissionServiceProvider"
            ],
            "aliases": {
                "RolesPermission": "Codenomads\\RolesPermission\\Facades\\RolesPermission"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "php-cs-fixer fix --allow-risky=yes",
        "analyse": "phpstan analyse"
    }
}
